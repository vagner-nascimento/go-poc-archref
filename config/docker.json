{
  "data": {
    "noSql": {
      "mongo": {
        "connStr": "mongodb://admin:admin@go-mongodb:27017",
        "clientTimeOut": 15,
        "insertTimeout": 5,
        "findTimeout": 8,
        "maxPaginatedSearch": 100,
        "database": "docker-golang"
      },
      "collections": {
        "customer": "customer",
        "supplier": "supplier"
      }
    },
    "amqp": {
      "connStr": "amqp://guest:guest@localhost:5672",
      "connectionRetry": {
        "sleep": 5,
        "maxTries": 5
      }
    }
  },
  "presentation": {
    "web": {
      "port": 3000
    }
  },
  "integration": {
    "amqp": {
      "subs": {
        "user": {
          "topic": "q-user",
          "consumer": "c-user"
        },
        "enterprise": {
          "topic": "q-enterprise",
          "consumer": "c-enterprise"
        }
      },
      "pubs": {
        "customer": {
          "topic": "q-customer"
        },
        "supplier": {
          "topic": "q-supplier"
        }
      }
    }
  }
}